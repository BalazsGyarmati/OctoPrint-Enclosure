<table class="table table-striped table-hover table-condensed table-hover" id="settings_enclosure_inputs" style="">
    <thead>
      <tr>
        <th class="">Id</th>
        <th class="">Label</th>
        <th class="">Type</th>
        <th class="">GPIO</th>
        <th class="">{{ _('Action') }}</th>
      </tr>
    </thead>
    <tbody data-bind="foreach: $root.enclosureInputs">
      <tr data-bind="attr: {title: name}">
        <td class="" data-bind="text: index_id"></td>
        <td class="" data-bind="text: label"></td>
        <td class="" data-bind="text: input_type"></td>
        <!-- ko if: gpio.pin_name  -->
        <td class="" data-bind="text: gpio.pin_name"></td>
        <!-- /ko -->
        <!-- ko ifnot: (gpio.pin_name ) -->
        <td class="" >N/A</td>
        <!-- /ko -->
        <td class="">
          <a href="#" class="fa fa-pencil" title="{{ _('Edit Output')|edq }}" data-bind="click: function() { $root.showInputEditorDialog($data); }"></a>&nbsp;|&nbsp;
          <a href="#" class="fa fa-trash-o" title="{{ _('Delete Output')|edq }}" data-bind="click: function() { $root.removeInput($data); }"></a>
      </td>
      </tr>
    </tbody>
  </table>
  
  <div class="control-group">
    <button class="btn pull-right" data-bind="click: function() { $root.showInputEditorDialog(); }">Add
      Inputs...</button>
  </div>